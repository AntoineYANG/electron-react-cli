{
  "name": "espoir-cli",
  "version": "1.0.2",
  "description": "front-end scripts.",
  "espoirVersion": 0,
  "keywords": [
    "monorepo",
    "package manager",
    "install",
    "workspace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AntoineYANG/espoir.git",
    "directory": "packages/espoir-cli"
  },
  "author": "kanatayou",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0 || >=16"
  },
  "bugs": {
    "url": "https://github.com/AntoineYANG/espoir/issues"
  },
  "homepage": "https://github.com/AntoineYANG/espoir/blob/main/packages/espoir-cli/README.md",
  "main": "./bin/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "README-*.md",
    "CHANGELOG-*.md",
    "/bin/",
    "lib",
    "public"
  ],
  "preferGlobal": true,
  "bin": {
    "espoir": "./bin/index.js"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": [
      {
        "default": "./bin/index.js"
      },
      "./bin/index.js"
    ],
    "./package.json": "./package.json",
    "./env": [
      {
        "default": "./bin/utils/env.js"
      },
      "./bin/utils/env.js"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "compressing": "^1.5.1",
    "enquirer": "^2.3.6",
    "inquirer": "^8.2.0",
    "listr2": "^3.14.0",
    "log-update": "^4.0.0",
    "mkdirp": "^1.0.4",
    "needle": "^3.0.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@jest/globals": "^27.4.6",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^8.1.3",
    "@types/mkdirp": "^1.0.2",
    "@types/needle": "^2.5.2",
    "@types/node": "^17.0.9",
    "@types/semver": "^7.3.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "jest": "^27.4.7",
    "jest-circus": "^27.4.6",
    "jest-config": "^27.4.7",
    "nodemon": "^2.0.15",
    "source-map-support": "^0.5.21",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "tsc",
    "test": "npx jest",
    "dev": "nodemon -w ./src/**/*.ts -e ts --exec \"espoir run espoir-cli.build\"",
    "publish": "npm publish --registry https://registry.npmjs.org"
  }
}
